  "scripts": {
    "start": "node client/npm-scripts/start.js",
    "build": "npm-run-all build-client move-to-public",
    "build-client": "node client/npm-scripts/build.js",
    "move-to-public": "rm -rf public && mv build public",
    "less": "node_modules/.bin/lessc client/src/less/main.less client/src/.css/main.css -x",
    "test": "node client/npm-scripts/test.js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@types/dompurify": "^0.0.33",
    "@types/showdown": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.7",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-watch-compiler": "^1.13.0",
    "mustache-loader": "^1.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.16.4",
    "raw-loader": "^4.0.0",
    "react-less-watcher": "^1.1.4",
    "spritesmith-cli": "^1.1.2",
    "text-transform-loader": "^2.0.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "worker-loader": "^2.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx,ts,tsx}",
      "!client/src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "<rootDir>/client/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/client/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/client/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/client/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/client/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "node_modules\\jest-watch-typeahead\\filename.js",
      "node_modules\\jest-watch-typeahead\\testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
